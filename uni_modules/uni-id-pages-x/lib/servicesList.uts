const loginTypes = config.getArray<string>('loginTypes')
servicesList = servicesList.filter((item) : boolean => {
  // #ifndef APP
  //非app端去掉apple登录
  if (item.id == 'apple') {
    return false
  }
  // #endif

  // #ifdef APP
  //去掉非ios系统上的apple登录
  if (item.id == 'apple' && uni.getSystemInfoSync().osName != 'ios') {
    return false
  }
  // #endif

  // 必须是配置了的
  return loginTypes!.includes(item.id)
})


